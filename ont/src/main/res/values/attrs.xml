<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LineChart">
        <attr name="grid_color" format="color" />
        <attr name="ylabel_color" format="color" />
        <attr name="xlabel_color" format="color" />
        <attr name="ylabel_text_size" format="dimension" />
        <attr name="xlabel_text_size" format="dimension" />
        <attr name="grid_row" format="integer" />
    </declare-styleable>

    <declare-styleable name="SpeedLineChart">
        <attr name="progress_line_color" format="color" />
        <attr name="progress_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="BarChart">
        <attr name="bar_color" format="color" />
    </declare-styleable>

    <declare-styleable name="PingChart" parent="BarChart">
        <attr name="bar_color" />
    </declare-styleable>

    <declare-styleable name="SignalBarChartView" parent="BarChart">
        <attr name="bar_color" />
    </declare-styleable>

    <declare-styleable name="ProgressBar">
        <attr name="horizontal_squares_count" format="integer" />
    </declare-styleable>
</resources>